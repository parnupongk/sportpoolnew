<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    tools:context=".SportPool_Program"
    android:background="@drawable/bg" >
    
    <ScrollView
            android:id="@+id/analysedetail_side_slide_layout"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical" >
            
        <RelativeLayout
                android:layout_width="fill_parent"
    			android:layout_height="wrap_content"
    			android:orientation="vertical" >
	
	<LinearLayout 
	    android:id="@+id/analysedetail_layout_analyse"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">
	    
	    <TextView android:id="@+id/analysedetail_txt_headerhandicap"
        	android:layout_width="fill_parent"
        	android:layout_height="wrap_content"
        	style="@style/TextGreenBold"/>
	    
	    <TextView android:id="@+id/analysedetail_txt_handicap"
        	android:layout_width="fill_parent"
        	android:layout_height="wrap_content"
        	style="@style/TextScore15"
        	android:layout_marginLeft="10dp"/>
	    
	    <TextView android:id="@+id/analysedetail_txt_headerdetail"
        	android:layout_width="fill_parent"
        	android:layout_height="wrap_content"
        	style="@style/TextGreenBold"/>
	    
	    <TextView android:id="@+id/analysedetail_txt_detail"
        	android:layout_width="fill_parent"
        	android:layout_height="wrap_content"
        	style="@style/TextScore15"
        	android:layout_marginLeft="10dp"/>
	    
	    <TextView android:id="@+id/analysedetail_txt_headerresult"
        	android:layout_width="fill_parent"
        	android:layout_height="wrap_content"
        	style="@style/TextGreenBold"/>
	    
	    <TextView android:id="@+id/analysedetail_txt_result"
        	android:layout_width="fill_parent"
        	android:layout_height="wrap_content"
        	style="@style/TextScore15"
        	android:layout_marginLeft="10dp"/>
	    
	</LinearLayout>      
	
	
	<RelativeLayout 
        android:id="@+id/sportpool_layout_header_htoh"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/analysedetail_layout_analyse"
        android:layout_centerVertical="true"
        android:background="@drawable/bg_white">
        
         <TextView android:id="@+id/analysedetail_txt_header_htoh"
        	android:layout_width="wrap_content"
        	android:layout_height="wrap_content"
        	android:layout_centerVertical="true"
        	style="@style/TextGreenBold"/>
         
    </RelativeLayout>
    
    	<RelativeLayout 
	    android:id="@+id/sportpool_layout_htoh"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/sportpool_layout_header_htoh"
        android:background="@drawable/bg_white">
	
		    <ImageView 
        android:id="@+id/analysedetail_img_staticteam1"
        android:layout_width="wrap_content"
  		android:layout_height="wrap_content"
  		android:layout_alignParentLeft="true"
  		android:layout_toLeftOf="@+id/analysedetail_layout_static"
  		/>
	
	<RelativeLayout 
	    android:id="@+id/analysedetail_layout_static"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
		android:layout_centerInParent="true"
        android:background="@drawable/bg_white">
	

	        	 <ListView
        android:id="@+id/analysedetail_list_static"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
         />

	
	</RelativeLayout>
	
		<ImageView 
        android:id="@+id/analysedetail_img_staticteam2"
        android:layout_width="wrap_content"
  		android:layout_height="wrap_content"
  		android:layout_alignParentRight="true"
  		android:layout_toRightOf="@+id/analysedetail_layout_static"/>
	
	</RelativeLayout>
	
	
			</RelativeLayout>
        
</ScrollView>
    	        	
    	        	
</RelativeLayout>