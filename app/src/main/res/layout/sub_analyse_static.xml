<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:layout_gravity="center_vertical"
    android:layout_marginTop="2dp"
    android:background="@drawable/bg_white">


    <LinearLayout
        android:id="@+id/subanalyse_layout_team1"
        android:layout_width="80dp"
        android:layout_height="wrap_content"
        android:layout_marginRight="1dp"
        android:layout_toLeftOf="@+id/subanalyse_layout_vs"
        android:gravity="right"
        android:orientation="vertical"></LinearLayout>

    <RelativeLayout
        android:id="@+id/subanalyse_layout_vs"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerInParent="true">

        <TextView
            android:id="@+id/subanalyse_txt_vs"
            style="@style/TextScoreSmall"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"></TextView>

    </RelativeLayout>

    <LinearLayout
        android:id="@+id/subanalyse_layout_team2"
        android:layout_width="80dp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="1dp"
        android:layout_toRightOf="@+id/subanalyse_layout_vs"
        android:orientation="vertical"></LinearLayout>


</RelativeLayout>
